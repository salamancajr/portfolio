  "private": true,
  "dependencies": {
    "arraybuffer-to-string": "^1.0.2",
    "axios": "^0.18.0",
    "http-proxy-middleware": "^0.19.1",
    "lodash": "^4.17.10",
    "marked": "^0.4.0",
    "react": "^16.8",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-redux-loading-bar": "^4.0.5",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "react-scripts": "1.1.4",
    "react-sortablejs": "^1.3.6",
    "react-thunk": "^1.0.0",
    "redux": "^4.0.0",
    "redux-form": "^7.4.2",
    "redux-promise": "^0.6.0",
    "redux-saga": "^1.0.2",
    "redux-thunk": "^2.3.0",
    "sortablejs": "^1.7.0",
    "webpack-dev-server": "^3.2.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build sw-precache --root='build/' --config config/sw.js ",
    "test": "jest",
    "eject": "react-scripts eject",
    "watch:sass": "node-sass src/sass/main.scss src/css/style.css -w",
    "compile:sass": "node-sass src/sass/main.scss src/css/style.comp.css",
    "concat:css": "concat -o src/css/style.concat.css src/css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' src/css/style.concat.css -o src/css/style.prefix.css",
    "compress:css": "node-sass src/css/style.prefix.css src/css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "devDependencies": {
    "autoprefixer": "^9.4.4",
    "chai": "^4.2.0",
    "concat": "^1.0.3",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "node-sass": "^4.9.2",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.1",
    "react-test-renderer": "^16.8.3"
  },
  "proxy": {
    "/api": {
      "target": "http://localhost:8082/",
      "secure": false
    }
  }
}
