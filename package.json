{
  "name": "portfolio_api",
  "version": "1.0.0",
  "engines": {
    "node": "11.11.0"
  },
  "description": "",
  "main": "server.js",
  "scripts": {
    "test-ci": "node --expose-internals ./node_modules/.bin/mocha **/*.test.js --exit",
    "test": "env-cmd -e test node --expose-internals ./node_modules/.bin/mocha **/*.test.js --exit",
    "test-watch": "nodemon --exec 'npm test'",
    "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build",
    "start": "node server.js",
    "build": "cd client && npm run build",
    "start-prod": "env-cmd -e production node server.js",
    "mongo": "cd ~/mongo/bin && ./mongod --dbpath ~/mongo-data",
    "client": "cd client && npm run start",
    "sass": "cd client && npm run watch:sass",
    "compile:sass": "cd client && node-sass sass/main.scss src/index.css -w",
    "all": "concurrently \"env-cmd -e development nodemon run start\" \"npm run client\" \"npm run sass\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.592.0",
    "bcryptjs": "^2.4.3",
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "crypto-js": "^3.1.9-1",
    "env-cmd": "^10.0.1",
    "express": "^4.16.4", 
    "jsonwebtoken": "^8.5.0",
    "lodash": "^4.17.19",
    "moment": "^2.24.0",
    "mongodb": "^3.1.13",
    "mongoose": "^5.7.5",
    "redis": "^2.8.0",
    "request": "^2.88.0",
    "simple-git": "^1.129.0",
    "validator": "^10.11.0"
  },
  "devDependencies": {
    "chalk": "^3.0.0",
    "eslint": "^6.7.2",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "expect": "^25.0.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.10",
    "puppeteer": "^2.0.0",
    "simple-git": "^1.129.0",
    "supertest": "^3.4.2"
  }
}
