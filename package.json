{
  "name": "portfolio_api",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "test": "export NODE_ENV=test || SET \"NODE_ENV=test\" && mocha **/*.test.js",
    "test-watch": "nodemon --exec 'npm test'",
    "start": "node server.js",
    "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build",
    "mongo": "cd ~/mongo/bin && ./mongod --dbpath ~/mongo-data",
    "client": "cd client && npm run start",
    "compile:sass": "cd client && node-sass sass/main.scss src/index.css -w",
    "all": "concurrently \"npm run mongo\" \"nodemon run start\" \"npm run client\" \"npm run compile:sass\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "concurrently": "^4.1.0",
    "cors": "^2.8.4",
    "crypto-js": "^3.1.9-1",
    "express": "^4.16.3",
    "image-downloader": "^3.4.1",
    "imagemin": "^6.1.0",
    "imagemin-jpegtran": "^6.0.0",
    "imagemin-pngquant": "^6.0.1",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "mongodb": "^3.1.0",
    "mongoose": "^5.2.1",
    "multer": "^1.3.1",
    "request": "^2.87.0",
    "validator": "^10.4.0"
  },
  "devDependencies": {
    "expect": "^23.4.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.3",
    "supertest": "^3.1.0"
  }
}
